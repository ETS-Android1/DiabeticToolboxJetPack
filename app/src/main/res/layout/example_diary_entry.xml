<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="4dp">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="4dp"
        app:layout_constraintTop_toTopOf="parent" >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="4dp">

            <TextView
                android:id="@+id/diary_text_meal_summary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:text="MealSummary"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/diary_text_carbs_exchanger"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="4dp"
                android:text="WW:"
                app:layout_constraintBaseline_toBaselineOf="@+id/diary_text_carbs_exchanger_summary_value"
                app:layout_constraintEnd_toStartOf="@+id/diary_text_carbs_exchanger_summary_value" />

            <TextView
                android:id="@+id/diary_text_protein_fat_exchanger"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="4dp"
                android:text="WBT:"
                app:layout_constraintBaseline_toBaselineOf="@+id/diary_text_protein_fat_exchanger_summary_value"
                app:layout_constraintEnd_toStartOf="@+id/diary_text_protein_fat_exchanger_summary_value" />

            <TextView
                android:id="@+id/diary_text_meal_fat_summary"
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:layout_marginEnd="4dp"
                android:text="@string/macro_fat"
                app:layout_constraintBaseline_toBaselineOf="@+id/diary_text_fat_summary_value"
                app:layout_constraintEnd_toStartOf="@+id/diary_text_fat_summary_value" />

            <TextView
                android:id="@+id/diary_text_meal_proteins_summary"
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:layout_marginEnd="4dp"
                android:text="@string/macro_proteins"
                app:layout_constraintBaseline_toBaselineOf="@+id/diary_text_proteins_summary_value"
                app:layout_constraintEnd_toStartOf="@+id/diary_text_proteins_summary_value" />

            <TextView
                android:id="@+id/diary_text_meal_carbs_summary"
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:layout_marginEnd="4dp"
                android:text="@string/macro_carbs"
                app:layout_constraintBaseline_toBaselineOf="@+id/diary_text_carbs_summary_value"
                app:layout_constraintEnd_toStartOf="@+id/diary_text_carbs_summary_value" />

            <TextView
                android:id="@+id/diary_text_calories_summary"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="4dp"
                android:text="Calories:"
                app:layout_constraintBaseline_toBaselineOf="@+id/diary_text_calories_summary_value"
                app:layout_constraintEnd_toStartOf="@+id/diary_text_calories_summary_value" />

            <TextView
                android:id="@+id/diary_text_carbs_summary_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:text="0"
                app:layout_constraintBaseline_toBaselineOf="@+id/diary_text_calories_summary"
                app:layout_constraintEnd_toStartOf="@+id/diary_text_calories_summary" />

            <TextView
                android:id="@+id/diary_text_carbs_exchanger_summary_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:text="999"
                app:layout_constraintBottom_toTopOf="@+id/diary_text_protein_fat_exchanger_summary_value"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/diary_text_calories_summary_value"
                app:layout_constraintTop_toBottomOf="@+id/diary_text_calories_summary_value" />

            <TextView
                android:id="@+id/diary_text_fat_summary_value"
                android:layout_width="wrap_content"
                android:layout_height="19dp"
                android:layout_marginEnd="16dp"
                android:text="0"
                app:layout_constraintBaseline_toBaselineOf="@+id/diary_text_carbs_exchanger"
                app:layout_constraintEnd_toStartOf="@+id/diary_text_calories_summary" />

            <TextView
                android:id="@+id/diary_text_proteins_summary_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:text="0"
                app:layout_constraintBaseline_toBaselineOf="@+id/diary_text_protein_fat_exchanger"
                app:layout_constraintEnd_toStartOf="@+id/diary_text_calories_summary" />

            <TextView
                android:id="@+id/diary_text_protein_fat_exchanger_summary_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="8dp"
                android:layout_marginBottom="8dp"
                android:text="999"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@+id/diary_text_carbs_exchanger_summary_value" />

            <TextView
                android:id="@+id/diary_text_calories_summary_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:text="999"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>